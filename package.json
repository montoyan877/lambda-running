{
  "name": "lambda-running",
  "version": "0.2.2",
  "description": "Library for running and testing AWS Lambda functions locally with custom events",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest",
    "test:ui": "jest --config tests/ui/jest.config.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "build": "babel src -d lib",
    "build:ui": "cd ui && npm run build",
    "clean:dist": "node scripts/clean-dist.js",
    "build:package": "npm run build && npm run build:ui && npm run clean:dist",
    "release": "npm run build:package && node scripts/release.js",
    "dev:tswatch": "tsc --watch",
    "ui:dev": "cd ui && npm run dev",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build:package"
  },
  "bin": {
    "lambda-run": "bin/lambda-run.js"
  },
  "keywords": [
    "aws",
    "lambda",
    "testing",
    "serverless",
    "debug",
    "runner",
    "running"
  ],
  "author": "Nicol√°s Montoya <montoyan877@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/montoyan877/lambda-running.git"
  },
  "bugs": {
    "url": "https://github.com/montoyan877/lambda-running/issues"
  },
  "homepage": "https://github.com/montoyan877/lambda-running#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "dotenv": "^16.0.2",
    "enquirer": "^2.4.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "open": "^8.4.2",
    "socket.io": "^4.7.2",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@types/node": "^18.7.23",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-vue": "^10.0.0",
    "jest": "^29.6.2",
    "prettier": "^3.5.3",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "bin/",
    "lib/",
    "README.md"
  ]
}
