    "inquirer": "^7.3.3",
    "prompts": "^2.4.2",
    "ts-node": "^10.9.2"
  },
  "optionalDependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "open": "^8.4.2",
    "socket.io": "^4.7.2",
